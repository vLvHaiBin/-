<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:setting="http://schemas.android.com/apk/res/com.example.mobilesafe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <com.example.mobilesafe.ViewGroup.Titlebar
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        setting:title="设置中心" >
    </com.example.mobilesafe.ViewGroup.Titlebar>

    <com.example.mobilesafe.ViewGroup.SettingRelativeLayout
        android:id="@+id/sr_update_setting"
        setting:name="auto_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        setting:desc_off="自动更新关闭"
        setting:desc_on="自动更新开启"
        setting:title="自动更新设置" >
    </com.example.mobilesafe.ViewGroup.SettingRelativeLayout>

    <com.example.mobilesafe.ViewGroup.SettingRelativeLayout
        android:id="@+id/sr_place_show"
        setting:name="place_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        setting:desc_off="归属地显示已关闭"
        setting:desc_on="归属地显示已开启"
        setting:title="电话归属地的显示设置" >
    </com.example.mobilesafe.ViewGroup.SettingRelativeLayout>

</LinearLayout>